<h1>Football teams WorldWide</h1>

<% @teams.each do |team| %>
    <%= image_tag team.image, :class =>'feature' if team.image.present? %>
        <ul>
            <li> Name: <%= link_to team.name, team %></li>
            <li>Country: <%= team.country %></li>
            <li>Founded: <%= team.dob%></li>
            
        
        </ul>
<% end%>