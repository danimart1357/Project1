<%= form_for @team do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :country %>
    <%= f.text_field :country %>

    <%= f.label :image%>
    <%= f.url_field :image %>

    <%= f.label :dob, "Date Founded" %>
    <%= f.text_field :dob, :placeholder => 'YYYY'%>
    <%= f.submit%>
<%end%>